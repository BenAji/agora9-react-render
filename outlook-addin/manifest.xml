<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" 
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
           xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" 
           xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" 
           xsi:type="TaskPaneApp">
  
  <!-- Begin Basic Settings: Add-in metadata, used for all versions of Office unless override provided. -->
  <Id>21a608a6-31e3-4767-aa3e-63d6f601ec55</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>AGORA Investment Calendar</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  
  <!-- The display name of your add-in. Used on the store and various places of the Office UI such as the add-ins dialog. -->
  <DisplayName DefaultValue="AGORA Investment Calendar" />
  <Description DefaultValue="Track corporate events, earnings calls, and conferences directly from Outlook. Sync your investment calendar with Outlook." />
  
  <!-- Icon for your add-in. Used on installation screens and the add-ins dialog. -->
  <IconUrl DefaultValue="https://agora9-react-render.onrender.com/static/media/logo192.png" />
  <HighResolutionIconUrl DefaultValue="https://agora9-react-render.onrender.com/static/media/logo512.png" />
  
  <!--If you plan to submit this add-in to the Office Store, uncomment the SupportUrl element below-->
  <SupportUrl DefaultValue="https://agora9-react-render.onrender.com" />
  
  <!-- Domains that will be allowed when navigating. For example, if you use ShowTaskpane and then have to do a redirect to a new page, then that new page must be included here. -->
  <AppDomains>
    <AppDomain>https://agora9-react-render.onrender.com</AppDomain>
  </AppDomains>
  
  <!--End Basic Settings. -->
  
  <!--Begin TaskPane Mode integration. This section is used if there are no VersionOverrides or if the Office client version does not support add-in commands. -->
  <Hosts>
    <Host Name="Mailbox" />
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://agora9-react-render.onrender.com/taskpane.html" />
  </DefaultSettings>
  <!-- End TaskPane Mode integration.  -->
  
  <Permissions>ReadWriteMailbox</Permissions>
  
  <!-- Begin Add-in Commands Mode integration. -->
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    
    <!-- The Hosts node is required. -->
    <Hosts>
      <!-- Each host can have a different set of commands. -->
      <!-- Outlook host: Mail, Calendar, Meeting Request, Read, or Compose -->
      <Host xsi:type="MailHost">
        
        <!-- Form factor. Currently only DesktopFormFactor is supported. -->
        <DesktopFormFactor>
          <!-- "This code enables a customizable message to be displayed when the add-in is loaded successfully upon individual install." -->
          <FunctionFile resid="Commands.Url" />
          
          <!-- PrimaryCommandSurface is the main Office Ribbon. -->
          <ExtensionPoint xsi:type="MessageReadCommandSurface">
            <!-- Use OfficeTab to extend an existing Tab. Use CustomTab to create a new tab. -->
            <OfficeTab id="TabDefault">
              <!-- Up to 6 Groups added per Tab. -->
              <Group id="msgReadGroup">
                <Label resid="GroupLabel" />
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                
                <!-- Control. It can be of type "Button" or "Menu". -->
                <Control xsi:type="Button" id="msgReadOpenPaneButton">
                  <Label resid="TaskPaneButton.Label" />
                  <Supertip>
                    <Title resid="TaskPaneButton.Label" />
                    <Description resid="TaskPaneButton.Tooltip" />
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  
                  <!-- This is what happens when the command is triggered (E.g. click on the Ribbon). Supported actions are ExecuteFunction or ShowTaskpane. -->
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <!-- Provide a url resource id for the location that will be opened on any client. -->
                    <SourceLocation resid="TaskPane.Url" />
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    
    <!-- You can use resources across hosts and form factors. -->
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://agora9-react-render.onrender.com/static/media/logo192.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://agora9-react-render.onrender.com/static/media/logo192.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://agora9-react-render.onrender.com/static/media/logo512.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://agora9-react-render.onrender.com" />
        <bt:Url id="Commands.Url" DefaultValue="https://agora9-react-render.onrender.com/commands.html" />
        <bt:Url id="TaskPane.Url" DefaultValue="https://agora9-react-render.onrender.com/taskpane.html" />
      </bt:Urls>
      <!-- ShortStrings max characters==125. -->
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="Get started with AGORA!" />
        <bt:String id="GroupLabel" DefaultValue="AGORA Calendar" />
        <bt:String id="TaskPaneButton.Label" DefaultValue="AGORA Calendar" />
      </bt:ShortStrings>
      <!-- LongStrings max characters==250. -->
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="Your sample add-in loaded successfully. Go to the HOME tab and click the 'Show Taskpane' button to get started." />
        <bt:String id="TaskPaneButton.Tooltip" DefaultValue="Open AGORA Investment Calendar" />
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
  <!-- End Add-in Commands Mode integration. -->
</OfficeApp>
